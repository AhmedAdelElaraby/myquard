<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ProfileCompany">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_backing"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="@dimen/_15sdp"
            android:layout_marginStart="@dimen/_15sdp"
            android:id="@+id/back" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="@dimen/_120sdp"
            android:layout_height="@dimen/_120sdp"
            android:elevation="@dimen/_5sdp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/imageprofile"
            android:layout_marginTop="@dimen/_70sdp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/_260sdp"
            android:background="@drawable/stilye_button_info"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/imageprofile"
            android:layout_marginTop="@dimen/_80sdp"
            android:layout_marginStart="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_5sdp"
            android:id="@+id/textback" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/seting"
            app:layout_constraintEnd_toEndOf="@id/textback"
            app:layout_constraintTop_toTopOf="@id/textback"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_5sdp"
            android:id="@+id/seting" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/imageprofile"
            android:text=""
            app:layout_constraintStart_toStartOf="@id/textback"
            app:layout_constraintEnd_toEndOf="@id/textback"
            android:layout_marginTop="@dimen/_10sdp"
            android:fontFamily="@font/notolkeaarabic_bold"
            android:textSize="@dimen/_25sdp"
            android:textStyle="bold"
            style="@style/text"
            android:id="@+id/name" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/name"
            android:text=""


            app:layout_constraintStart_toStartOf="@id/textback"
            app:layout_constraintEnd_toEndOf="@id/textback"
            android:layout_marginTop="@dimen/_2sdp"
            android:fontFamily="@font/notolkeaarabic_bold"
            android:textSize="@dimen/_10sdp"
            android:textStyle="bold"
            android:textColor="#13718D"
            android:id="@+id/typecompany" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/typecompany"
            android:text=" "


            app:layout_constraintStart_toStartOf="@id/textback"
            app:layout_constraintEnd_toEndOf="@id/textback"
            android:layout_marginTop="@dimen/_5sdp"
            android:fontFamily="@font/notolkeaarabic_bold"
            android:textSize="@dimen/_10sdp"
            android:textStyle="bold"
            android:textColor="#13718D"
            android:id="@+id/location" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#13718D"
            android:layout_marginStart="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_5sdp"
            android:id="@+id/view"
            app:layout_constraintEnd_toEndOf="@id/textback"
            app:layout_constraintStart_toStartOf="@id/textback"
            app:layout_constraintTop_toBottomOf="@id/location"
            android:layout_marginTop="@dimen/_20sdp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/commercial_registration_no"
            app:layout_constraintTop_toBottomOf="@+id/view"
            app:layout_constraintStart_toStartOf="@id/textback"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:textSize="@dimen/_9sdp"
            android:fontFamily="@font/notolkeaarabic_bold"
            style="@style/text"
            android:id="@+id/textemail" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/view"
            app:layout_constraintEnd_toEndOf="@id/textback"
            android:text=""
            android:layout_marginEnd="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:textSize="@dimen/_9sdp"
            android:fontFamily="@font/notolkeaarabic_bold"
            android:textColor="#13718D"
            android:id="@+id/email" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#13718D"
            android:layout_marginStart="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_5sdp"
            android:id="@+id/viewone"
            app:layout_constraintEnd_toEndOf="@id/textback"
            app:layout_constraintStart_toStartOf="@id/textback"
            app:layout_constraintTop_toBottomOf="@id/email"
            android:layout_marginTop="@dimen/_5sdp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="number Company"
            app:layout_constraintStart_toStartOf="@id/textback"
            app:layout_constraintTop_toBottomOf="@id/viewone"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:textSize="@dimen/_10sdp"
            android:fontFamily="@font/notolkeaarabic_bold"
            style="@style/text"
            android:id="@+id/textAcuontBink" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="@id/textback"
            app:layout_constraintTop_toBottomOf="@id/viewone"
            android:text=""
            android:layout_marginEnd="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:textSize="@dimen/_10sdp"
            android:fontFamily="@font/notolkeaarabic_bold"
            android:textColor="#13718D"
            android:id="@+id/AcuontBank" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#13718D"
            android:layout_marginStart="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_5sdp"
            android:id="@+id/viewtwo"
            app:layout_constraintEnd_toEndOf="@id/textback"
            app:layout_constraintStart_toStartOf="@id/textback"
            app:layout_constraintTop_toBottomOf="@id/textAcuontBink"
            android:layout_marginTop="@dimen/_5sdp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Email Company"
            app:layout_constraintStart_toStartOf="@id/textback"
            app:layout_constraintTop_toBottomOf="@id/viewtwo"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:textSize="@dimen/_10sdp"
            android:fontFamily="@font/notolkeaarabic_bold"
            style="@style/text"
            android:id="@+id/textemailmon" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="@id/textback"
            app:layout_constraintTop_toBottomOf="@id/viewtwo"
            android:text=""
            android:layout_marginEnd="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:textSize="@dimen/_10sdp"
            android:fontFamily="@font/notolkeaarabic_bold"
            android:textColor="#13718D"
            android:id="@+id/emailmon" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>