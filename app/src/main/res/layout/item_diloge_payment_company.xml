<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@android:color/transparent"
    android:layout_height="match_parent"
    android:id="@+id/c1"
    xmlns:app="http://schemas.android.com/apk/res-auto">
<ImageView
    android:paddingBottom="@dimen/_3sdp"
    android:paddingTop="@dimen/_3sdp"
    android:layout_width="@dimen/_50sdp"
    android:layout_height="@dimen/_50sdp"
    android:background="@drawable/stilydiloge"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    android:src="@drawable/logo_splash_dark"
    android:id="@+id/image"
    android:elevation="@dimen/_5sdp"
    app:layout_constraintTop_toTopOf="parent"/>
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/stilydiloge"
    app:layout_constraintTop_toTopOf="@id/image"
    android:layout_marginTop="@dimen/_45sdp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"

    tools:ignore="MissingConstraints">
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/reced"
    android:layout_marginTop="@dimen/_5sdp"
    android:textSize="@dimen/_15sdp"
    android:textColor="@color/black"
    android:fontFamily="@font/notolkeaarabic_bold"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="parent"

    android:id="@+id/text"/>
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textSize="@dimen/_10sdp"
    android:text="@string/intredaction"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    android:gravity="center"
    android:textColor="@color/black"
    app:layout_constraintTop_toBottomOf="@id/text"
    android:layout_marginTop="@dimen/_2sdp"
    android:id="@+id/textone"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1sdp"
        android:background="#13718D"
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_marginEnd="@dimen/_10sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textone"
        android:layout_marginTop="@dimen/_5sdp"
        android:id="@+id/viewone"/>
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/single_package"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@id/viewone"
    android:layout_marginTop="@dimen/_5sdp"
    android:layout_marginStart="@dimen/_10sdp"
    android:textColor="#13718D"
    android:fontFamily="@font/notolkeaarabic_bold"
    android:textSize="@dimen/_15sdp"
    android:id="@+id/namePackage"/>
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="150 SR"
    android:textColor="@color/black"
    android:fontFamily="@font/notolkeaarabic_bold"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@id/viewone"
    android:layout_marginEnd="@dimen/_10sdp"
    android:layout_marginTop="@dimen/_10sdp"
    android:textSize="@dimen/_11sdp"

    android:id="@+id/packagepric"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/the_number"
        android:id="@+id/the_number"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/namePackage"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginStart="@dimen/_10sdp"
        android:textColor="#13718D"
        android:fontFamily="@font/notolkeaarabic_bold"
        android:textSize="@dimen/_15sdp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="15"
        android:textColor="@color/black"
        android:fontFamily="@font/notolkeaarabic_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/packagepric"
        android:layout_marginEnd="@dimen/_15sdp"
        android:layout_marginTop="@dimen/_15sdp"
        android:textSize="@dimen/_11sdp"
        android:id="@+id/number"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1sdp"
        android:background="#13718D"
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_marginEnd="@dimen/_10sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/number"
        android:layout_marginTop="@dimen/_10sdp"
        android:id="@+id/viewtwo"/>
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@id/viewtwo"
    android:text="@string/single_price"
    android:layout_marginTop="@dimen/_180sdp"
    android:textSize="@dimen/_15sdp"
    android:textColor="#13718D"
    android:fontFamily="@font/notolkeaarabic_bold"
    android:id="@+id/Single_price"
    android:layout_marginStart="@dimen/_10sdp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/viewtwo"
        android:text="10 SR"
        android:layout_marginTop="@dimen/_180sdp"
        android:textSize="@dimen/_15sdp"
       android:textColor="@color/black"
        android:fontFamily="@font/notolkeaarabic_bold"
        android:id="@+id/numberprice"
        android:layout_marginEnd="@dimen/_10sdp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/numberprice"
        android:text="@string/the_required_number"
        android:layout_marginTop="@dimen/_5sdp"
        android:textSize="@dimen/_15sdp"
        android:textColor="#13718D"
        android:fontFamily="@font/notolkeaarabic_bold"
        android:id="@+id/The_required_number"
        android:layout_marginStart="@dimen/_10sdp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/numberprice"
        android:text="10 SR"
        android:layout_marginTop="@dimen/_5sdp"
        android:textSize="@dimen/_15sdp"
        android:textColor="@color/black"
        android:fontFamily="@font/notolkeaarabic_bold"
        android:id="@+id/numberrequired"
        android:layout_marginEnd="@dimen/_10sdp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/The_required_number"
        android:text="@string/total"
        android:layout_marginTop="@dimen/_5sdp"
        android:textSize="@dimen/_15sdp"
        android:textColor="#13718D"
        android:fontFamily="@font/notolkeaarabic_bold"
        android:id="@+id/total"
        android:layout_marginStart="@dimen/_10sdp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/numberrequired"
        android:text="10 SR"
        android:layout_marginTop="@dimen/_5sdp"
        android:textSize="@dimen/_15sdp"
        android:textColor="@color/black"
        android:fontFamily="@font/notolkeaarabic_bold"
        android:id="@+id/numbertotal"
        android:layout_marginEnd="@dimen/_10sdp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/total"
        android:text="@string/tax_15"
        android:layout_marginTop="@dimen/_5sdp"
        android:textSize="@dimen/_15sdp"
        android:textColor="#13718D"
        android:fontFamily="@font/notolkeaarabic_bold"
        android:id="@+id/TaxText"
        android:layout_marginStart="@dimen/_10sdp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/numbertotal"
        android:text="10 SR"
        android:layout_marginTop="@dimen/_5sdp"
        android:textSize="@dimen/_15sdp"
        android:textColor="@color/black"
        android:fontFamily="@font/notolkeaarabic_bold"
        android:id="@+id/numberTax"
        android:layout_marginEnd="@dimen/_10sdp"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1sdp"
        android:background="#13718D"
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_marginEnd="@dimen/_10sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/numberTax"
        android:layout_marginTop="@dimen/_5sdp"
        android:id="@+id/viewthree"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/viewthree"
        android:text="@string/total"
        android:layout_marginTop="@dimen/_5sdp"
        android:textSize="@dimen/_15sdp"
        android:textColor="#13718D"
        android:fontFamily="@font/notolkeaarabic_bold"
        android:id="@+id/total_Full"
        android:layout_marginStart="@dimen/_10sdp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/viewthree"
        android:text="10 SR"
        android:layout_marginTop="@dimen/_5sdp"
        android:textSize="@dimen/_15sdp"
        android:textColor="@color/black"
        android:fontFamily="@font/notolkeaarabic_bold"
        android:id="@+id/numberFull"
        android:layout_marginEnd="@dimen/_10sdp"/>
    <com.google.android.material.button.MaterialButton
        android:id="@+id/bay"
        android:layout_width="@dimen/_250sdp"
        android:layout_height="57dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="@dimen/_3sdp"
        android:background="@drawable/style_buttonsignup"
        android:elevation="3dp"
        android:fontFamily="@font/notolkeaarabic_bold"
        android:includeFontPadding="false"
        android:text="@string/pay"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:layout_marginTop="@dimen/_10sdp"
        android:textStyle="bold"
        app:backgroundTint="@null"
        app:backgroundTintMode="add"
        app:layout_constraintTop_toBottomOf="@+id/numberFull"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>